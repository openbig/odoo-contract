<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
    	<record id="view_config_settings_licenses" model="ir.ui.view">
            <field name="model">sale.config.settings</field>
            <field name="inherit_id" ref="sale.view_sales_config"/>
            <field name="arch" type="xml">
            	<separator string="Contracts Management" position="after">
            		<group colspan="6">
                        <label for="id" string="License Generation"/>
                        <div>
                            <div>
                                <field name="backend_address" class="oe_inline"/>
                                <label for="backend_address"/>
                            </div>
                            <div>
                            	<field name="license_gen_login" class="oe_inline"/>
                            	<label for="license_gen_login"/>
                            </div>
                            <div>
                            	<field name="license_gen_pass" class="oe_inline"/>
                            	<label for="license_gen_pass"/>
                            </div>
                        </div>
                    </group>
                </separator>
            </field>
        </record>
    </data>
</openerp>